{
  "name": "remove-undefined-objects",
  "description": "Remove undefined properties and empty arrays from an object",
  "version": "3.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/remove-undefined-objects.git"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/ @types/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "tsc; jest --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@readme/eslint-config": "^10.6.0",
    "@types/jest": "^29.5.2",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
